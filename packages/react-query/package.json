{
  "name": "@srpc.org/react-query",
  "version": "0.20.2",
  "description": "React Query integration for SRPC - Type-safe RPC with automatic React Query hooks",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bunchee",
    "test": "vitest run",
    "test:dev": "vitest",
    "version": "node ../../version.mjs",
    "release:npm": "npm publish",
    "release:jsr": "pnpx jsr publish"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./context": {
      "types": "./dist/context.d.ts",
      "default": "./dist/context.js"
    }
  },
  "keywords": [
    "rpc",
    "typescript",
    "react-query",
    "tanstack-query",
    "type-safe",
    "hooks"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@srpc.org/core": "workspace:*",
    "@tanstack/react-query": "^5.90.10",
    "@types/react": "^19.2.6",
    "bunchee": "^6.6.2",
    "get-port-please": "^3.1.2",
    "react": "^19.2.0",
    "vitest": "^4.0.10"
  },
  "peerDependencies": {
    "@srpc.org/core": "workspace:*",
    "@tanstack/react-query": ">=5.90",
    "react": ">=19"
  }
}
